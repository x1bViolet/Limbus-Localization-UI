<Window
	x:Class="LC_Localization_Task_Absolute.Json.SkillsDisplayInfoManagerWindow"
	xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
	xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
	xmlns:Icons="clr-namespace:Visual_Studio_controls.Icons"
	xmlns:Terminus="clr-namespace:LC_Localization_Task_Absolute"
	xmlns:local="clr-namespace:LC_Localization_Task_Absolute.Json"
	Title="Skills Display Info manager (Very serious)"
	Height="510"
	MinWidth="571"
	MinHeight="510"
	MaxWidth="571"
	Loaded="Window_Loaded"
	Topmost="True"
	WindowStartupLocation="CenterScreen">
	<Window.Resources>
		<ResourceDictionary>
			<ResourceDictionary.MergedDictionaries>
				<ResourceDictionary Source="/Json/Skills Display Info manager/(Serious area do not enter without caution)/Visual Studio controls/Border.xaml" />
				<ResourceDictionary Source="/Json/Skills Display Info manager/(Serious area do not enter without caution)/Visual Studio controls/ScrollBar.xaml" />
				<ResourceDictionary Source="/Json/Skills Display Info manager/(Serious area do not enter without caution)/Visual Studio controls/ScrollViewer.xaml" />
				<ResourceDictionary Source="/Json/Skills Display Info manager/(Serious area do not enter without caution)/Visual Studio controls/ContextMenu.xaml" />
				<ResourceDictionary Source="/Json/Skills Display Info manager/(Serious area do not enter without caution)/Visual Studio controls/Button.xaml" />
				<ResourceDictionary Source="/Json/Skills Display Info manager/(Serious area do not enter without caution)/Visual Studio controls/ComboBox.xaml" />
				<ResourceDictionary Source="/Json/Skills Display Info manager/(Serious area do not enter without caution)/Visual Studio controls/ComboBoxItem.xaml" />
				<ResourceDictionary Source="/Json/Skills Display Info manager/(Serious area do not enter without caution)/Visual Studio controls/TextBox.xaml" />
				<ResourceDictionary Source="/Json/Skills Display Info manager/(Serious area do not enter without caution)/Visual Studio controls/CheckBox.xaml" />
				<ResourceDictionary Source="/Json/Skills Display Info manager/(Serious area do not enter without caution)/Visual Studio controls/TabControl.xaml" />
				<ResourceDictionary Source="/Json/Skills Display Info manager/(Serious area do not enter without caution)/Visual Studio controls/TabItem.xaml" />
			</ResourceDictionary.MergedDictionaries>
		</ResourceDictionary>
	</Window.Resources>
	<Grid Background="#1f1f1f" Margin="0 0 -0.2 -0.2">
		<Grid>
			<Grid>
				<Grid Height="23.96" Margin="5 5 5 5" VerticalAlignment="Top">
					<StackPanel Orientation="Horizontal">
						<Button Height="23.96" Click="LoadDisplayInfoFile">
							<Terminus:UITranslation_Rose
								Width="120"
								FontFamily="/UI/Fonts/#GOST type BU"
								FontSize="16.5"
								PerfectVerticalAlign="True"
								RichText="Open file"
								TextAlignment="Center"
								UID="[Skills DI Manager] * Open file button" />
						</Button>
						<Button Height="23.96" Margin="5 0 0 0" Click="SaveDisplayInfoFile">
							<Terminus:UITranslation_Rose
								Width="120"
								FontFamily="/UI/Fonts/#GOST type BU"
								FontSize="16.5"
								PerfectVerticalAlign="True"
								RichText="Save current"
								TextAlignment="Center"
								UID="[Skills DI Manager] * Save current button" />
						</Button>
						<Terminus:UITranslation_Rose
							x:Name="SelectedFileLabel"
							UID="[Skills DI Manager] * Current file name"
							HorizontalAlignment="Left"
							VerticalAlignment="Center"
							PerfectVerticalAlign="True"
							FontFamily="/UI/Fonts/#GOST type BU"
							FontSize="16"
							Foreground="#F5F5DC"
							MaxWidth="283"
							Margin="5 0 0 0"
							Text="···.json"
							TextTrimming="CharacterEllipsis"
							TextWrapping="NoWrap"
							ToolTipService.InitialShowDelay="0">
							<Terminus:UITranslation_Rose.ToolTip>
								<ToolTip Background="#E0676767" HorizontalOffset="20">
									<Terminus:UITranslation_Rose
										x:Name="SelectedFileLabel_Tooltip"
										UID="[Skills DI Manager] * Current file name (Full path tooltip)"
										FontFamily="/UI/Fonts/#GOST type BU"
										FontSize="14"
										Foreground="#F5F5DC"
										Text="···" />
								</ToolTip>
							</Terminus:UITranslation_Rose.ToolTip>
						</Terminus:UITranslation_Rose>
					</StackPanel>
				</Grid>

				<Grid x:Name="MainGrid" Margin="5 35 5 6">

					<Grid.ColumnDefinitions>
						<ColumnDefinition Width="197.85" />
						<ColumnDefinition />
					</Grid.ColumnDefinitions>

					<Grid.Resources>
						<PopupAnimation x:Key="{x:Static SystemParameters.MenuPopupAnimationKey}">None</PopupAnimation>

						<ContextMenu x:Key="TabItemContextMenu">
							<MenuItem FontSize="{Binding RelativeSource={RelativeSource Mode=Self}, Path=Header.FontSize}" Click="MoveTabItemUp">
								<MenuItem.Icon>
									<Icons:ArrowUp />
								</MenuItem.Icon>
								<MenuItem.Header>
									<Terminus:UITranslation_Rose
										UID="[Skills DI Manager] * Skill switch button (Context menu — Move up)"
										FontFamily="/UI/Fonts/#GOST type BU"
										FontSize="16.5"
										Foreground="#d6d6d6"
										Text="Move up" />
								</MenuItem.Header>
							</MenuItem>
							<MenuItem FontSize="{Binding RelativeSource={RelativeSource Mode=Self}, Path=Header.FontSize}" Click="DeleteTabItem">
								<MenuItem.Icon>
									<Icons:Delete />
								</MenuItem.Icon>
								<MenuItem.Header>
									<Terminus:UITranslation_Rose
										UID="[Skills DI Manager] * Skill switch button (Context menu — Delete)"
										FontFamily="/UI/Fonts/#GOST type BU"
										FontSize="16.5"
										Foreground="#d6d6d6"
										Text="Delete" />
								</MenuItem.Header>
							</MenuItem>
							<MenuItem FontSize="{Binding RelativeSource={RelativeSource Mode=Self}, Path=Header.FontSize}" Click="MoveTabItemDown">
								<MenuItem.Icon>
									<Icons:ArrowDown />
								</MenuItem.Icon>
								<MenuItem.Header>
									<Terminus:UITranslation_Rose
										UID="[Skills DI Manager] * Skill switch button (Context menu — Move down)"
										FontFamily="/UI/Fonts/#GOST type BU"
										FontSize="16.5"
										Foreground="#d6d6d6" 
										Text="Move down" />
								</MenuItem.Header>
							</MenuItem>
						</ContextMenu>

						<ContextMenu x:Key="TextfieldContextMenu">
							<ContextMenu.Items>
								<MenuItem FontSize="{Binding RelativeSource={RelativeSource Mode=Self}, Path=Header.FontSize}" Command="ApplicationCommands.Copy">
									<MenuItem.Icon>
										<Icons:Copy/>
									</MenuItem.Icon>
									<MenuItem.Header>
										<Terminus:UITranslation_Rose
											UID="[Skills DI Manager] * Textfields context menu — Copy"
											FontSize="16.5"
											Width="100"
											Foreground="#d6d6d6"
											Text="Copy" />
									</MenuItem.Header>
								</MenuItem>
								<MenuItem FontSize="{Binding RelativeSource={RelativeSource Mode=Self}, Path=Header.FontSize}" Command="ApplicationCommands.Cut">
									<MenuItem.Icon>
										<Icons:Cut/>
									</MenuItem.Icon>
									<MenuItem.Header>
										<Terminus:UITranslation_Rose
											UID="[Skills DI Manager] * Textfields context menu — Cut"
											FontSize="16.5"
											Width="100"
											Foreground="#d6d6d6"
											Text="Cut" />
									</MenuItem.Header>
								</MenuItem>
								<MenuItem FontSize="{Binding RelativeSource={RelativeSource Mode=Self}, Path=Header.FontSize}" Command="ApplicationCommands.Paste">
									<MenuItem.Icon>
										<Icons:Paste/>
									</MenuItem.Icon>
									<MenuItem.Header>
										<Terminus:UITranslation_Rose
											UID="[Skills DI Manager] * Textfields context menu — Paste"
											FontSize="16.5"
											Width="100"
											Foreground="#d6d6d6"
											Text="Paste" />
									</MenuItem.Header>
								</MenuItem>
							</ContextMenu.Items>
						</ContextMenu>

						<Style BasedOn="{StaticResource {x:Type TextBox}}" TargetType="{x:Type Terminus:UITranslation_Mint}">
							<Setter Property="Padding" Value="0 1 0 0" />
							<Setter Property="Height" Value="23.96" />
							<Setter Property="ContextMenu" Value="{DynamicResource TextfieldContextMenu}"/>
						</Style>
						<Style BasedOn="{StaticResource {x:Type ComboBox}}" TargetType="{x:Type ComboBox}">
							<Setter Property="Height" Value="23.96" />
						</Style>
						<Style BasedOn="{StaticResource {x:Type Button}}" TargetType="{x:Type Button}">
							<Setter Property="Height" Value="23.96" />
						</Style>
						<Style BasedOn="{StaticResource {x:Type TabItem}}" TargetType="{x:Type TabItem}">
							<Setter Property="ContextMenu" Value="{StaticResource TabItemContextMenu}" />
						</Style>
						<Style BasedOn="{StaticResource {x:Type CheckBox}}" TargetType="{x:Type CheckBox}">
							<Setter Property="Height" Value="23.96" />
							<Setter Property="Width" Value="23.96" />
							<Setter Property="HorizontalAlignment" Value="Left" />
							<Setter Property="VerticalAlignment" Value="Center" />
						</Style>
					</Grid.Resources>

					<Terminus:UITranslation_Rose
						UID="[Skills DI Manager] * Skill switch button"
						x:Name="TabItemHeaderTextBinding"
						Padding="0 3 0 3"
						FontSize="16"
						Width="148"
						PerfectVerticalAlign="True"
						VerticalAlignment="Center"
						TextWrapping="Wrap"
						Foreground="#D6D6D6"
						FontFamily="/UI/Fonts/#GOST type BU"
						Visibility="Collapsed"/>
					<TabControl x:Name="CurrentConstructorsList" Margin="0 0 0 29" SelectionChanged="UpdateCurrentConstructorView" />

					<Button
						x:Name="AddConstructorButton"
						Grid.ColumnSpan="2"
						Width="198"
						Height="24"
						Margin="0 0 0 0"
						Padding="5 -1 0 0"
						HorizontalAlignment="Left"
						VerticalAlignment="Bottom"
						HorizontalContentAlignment="Left"
						Click="AddConstructorButton_Click">
						<Terminus:UITranslation_Rose
							UID="[Skills DI Manager] * New skill constructor button"
							FontFamily="/UI/Fonts/#GOST type BU"
							FontSize="16.5"
							VerticalAlignment="Center"
							RichText="[b]+[/b] New skill constructor" />
					</Button>

					<ScrollViewer Grid.Column="1" Margin="5 0 0 0" VerticalScrollBarVisibility="Hidden">
						<StackPanel MinWidth="308.5">
							<Grid>
								<Grid.ColumnDefinitions>
									<ColumnDefinition />
									<ColumnDefinition />
								</Grid.ColumnDefinitions>
								<Terminus:UITranslation_Rose
									UID="[Skills DI Manager] * Skill name"
									VerticalAlignment="Center"
									PerfectVerticalAlign="True"
									FontFamily="/UI/Fonts/#GOST type BU"
									FontSize="20"
									Foreground="#F5F5DC"
									RichText="Skill name" />
								<Terminus:UITranslation_Mint
									UID="[Skills DI Manager] * Skill name"
									x:Name="SkillNameTextInput"
									Grid.Column="1"
									FontFamily="/UI/Fonts/#GOST type BU"
									FontSize="16.5"
									KeyDown="SkillNameTextInput_KeyDown"
									TextChanged="SkillNameTextInput_TextChanged" />
							</Grid>
							<Grid Margin="0 5 0 0">
								<Grid.ColumnDefinitions>
									<ColumnDefinition />
									<ColumnDefinition />
								</Grid.ColumnDefinitions>
								<Terminus:UITranslation_Rose
									UID="[Skills DI Manager] * Skill ID"
									VerticalAlignment="Center"
									PerfectVerticalAlign="True"
									FontFamily="/UI/Fonts/#GOST type BU"
									FontSize="20"
									Foreground="#F5F5DC"
									RichText="Skill ID" />
								<Terminus:UITranslation_Mint
									UID="[Skills DI Manager] * Skill ID"
									x:Name="SkillIDTextInput"
									Grid.Column="1"
									FontFamily="/UI/Fonts/#GOST type BU"
									FontSize="16.5"
									KeyDown="SkillIDTextInput_KeyDown"
									TextChanged="SkillIDTextInput_TextChanged" />
								<Border x:Name="SkillIDTextInput_InvalidIndicator" Grid.Column="1" BorderBrush="Transparent" />
							</Grid>
							<Grid Margin="0 5 0 0">
								<Grid.ColumnDefinitions>
									<ColumnDefinition />
									<ColumnDefinition />
								</Grid.ColumnDefinitions>
								<Terminus:UITranslation_Rose
									UID="[Skills DI Manager] * Icon image (Label)"
									VerticalAlignment="Center"
									PerfectVerticalAlign="True"
									x:Name="SelectSkillIconLabel"
									FontFamily="/UI/Fonts/#GOST type BU"
									FontSize="20"
									Foreground="#F5F5DC"
									RichText="Icon image [size=75%][color=#fc5a03][None][/size]"
									ToolTipService.InitialShowDelay="0">
									<Terminus:UITranslation_Rose.ToolTip>
										<ToolTip x:Name="SelectSkillIconLabel_Tooltip" Background="#E0676767" HorizontalOffset="20">
											<Terminus:UITranslation_Rose
												UID="[Skills DI Manager] * Icon image (Label) (Full path tooltip)"
												x:Name="SelectSkillIconLabel_Tooltip_Text"
												FontFamily="/UI/Fonts/#GOST type BU"
												FontSize="14"
												Foreground="#F5F5DC"
												RichText="···" />
										</ToolTip>
									</Terminus:UITranslation_Rose.ToolTip>
								</Terminus:UITranslation_Rose>
								<Button Grid.Column="1" Click="SelectSkillIcon" MouseRightButtonDown="ResetSelectedSkillIcon">
									<Terminus:UITranslation_Rose
										UID="[Skills DI Manager] * Icon image (Button)"
										FontFamily="/UI/Fonts/#GOST type BU"
										FontSize="16.5"
										VerticalAlignment="Center"
										PerfectVerticalAlign="True"
										RichText="Select image file" />
								</Button>
							</Grid>
							<StackPanel Margin="0 5 0 0">
								<StackPanel.Resources>
									<Style TargetType="{x:Type Image}">
										<Setter Property="Width" Value="19" />
										<Setter Property="Margin" Value="0 -1 0 0" />
									</Style>
								</StackPanel.Resources>
								<Grid>
									<Grid.ColumnDefinitions>
										<ColumnDefinition />
										<ColumnDefinition />
									</Grid.ColumnDefinitions>
									<Terminus:UITranslation_Rose
										UID="[Skills DI Manager] * Skill action (Label)"
										VerticalAlignment="Center"
										PerfectVerticalAlign="True"
										x:Name="SkillTypeSelectLabel"
										FontFamily="/UI/Fonts/#GOST type BU"
										FontSize="20"
										Foreground="#F5F5DC"
										RichText="Action" />
									<ComboBox x:Name="SkillTypeSelector" Grid.Column="1" SelectedIndex="0" SelectionChanged="SkillTypeSelector_SelectionChanged">
										<ComboBox.Resources>
											<Style TargetType="{x:Type Terminus:UITranslation_Rose}">
												<Setter Property="Foreground" Value="#F5F5DC" />
												<Setter Property="FontFamily" Value="{Binding ElementName=SkillTypeSelectLabel, Path=FontFamily}" />
												<Setter Property="FontWeight" Value="{Binding ElementName=SkillTypeSelectLabel, Path=FontWeight}" />
											</Style>
										</ComboBox.Resources>
										<StackPanel x:Uid="Attack" Orientation="Horizontal">
											<Image Source="/Json/Skills Display Info manager/(Serious area do not enter without caution)/Skill types/Attack.png" />
											<Terminus:UITranslation_Rose
												UID="[Skills DI Manager] * Skill action — Attack"
												FontSize="16.5"
												RichText="Attack" />
										</StackPanel>
										<StackPanel x:Uid="Counter" Orientation="Horizontal">
											<Image Source="/Json/Skills Display Info manager/(Serious area do not enter without caution)/Skill types/Counter.png" />
											<Terminus:UITranslation_Rose
												UID="[Skills DI Manager] * Skill action — Counter"
												FontSize="16.5"
												RichText="Counter" />
										</StackPanel>
										<StackPanel x:Uid="Guard" Orientation="Horizontal">
											<Image Source="/Json/Skills Display Info manager/(Serious area do not enter without caution)/Skill types/Guard.png" />
											<Terminus:UITranslation_Rose
												UID="[Skills DI Manager] * Skill action — Guard"
												FontSize="16.5"
												RichText="Guard" />
										</StackPanel>
										<StackPanel x:Uid="Evade" Orientation="Horizontal">
											<Image Source="/Json/Skills Display Info manager/(Serious area do not enter without caution)/Skill types/Evade.png" />
											<Terminus:UITranslation_Rose
												UID="[Skills DI Manager] * Skill action — Evade"
												FontSize="16.5"
												RichText="Evade" />
										</StackPanel>
									</ComboBox>
								</Grid>
								<Grid Margin="0 5 0 0">
									<Grid.ColumnDefinitions>
										<ColumnDefinition />
										<ColumnDefinition />
									</Grid.ColumnDefinitions>
									<Terminus:UITranslation_Rose
										UID="[Skills DI Manager] * Damage type (Label)"
										VerticalAlignment="Center"
										PerfectVerticalAlign="True"
										x:Name="DamageTypeSelectLabel"
										FontFamily="/UI/Fonts/#GOST type BU"
										FontSize="20"
										Foreground="#F5F5DC"
										RichText="Damage type" />
									<ComboBox x:Name="DamageTypeSelectSelector" Grid.Column="1" SelectedIndex="0" SelectionChanged="DamageTypeSelectSelector_SelectionChanged">
										<ComboBox.Resources>
											<Style TargetType="{x:Type Terminus:UITranslation_Rose}">
												<Setter Property="Foreground" Value="#F5F5DC" />
												<Setter Property="FontFamily" Value="{Binding ElementName=DamageTypeSelectLabel, Path=FontFamily}" />
												<Setter Property="FontWeight" Value="{Binding ElementName=DamageTypeSelectLabel, Path=FontWeight}" />
											</Style>
										</ComboBox.Resources>
										<StackPanel x:Uid="Blunt" Orientation="Horizontal">
											<Image Source="/Json/Skills Display Info manager/(Serious area do not enter without caution)/Damage types/Blunt.png" />
											<Terminus:UITranslation_Rose
												UID="[Skills DI Manager] * Damage type — Blunt"
												FontSize="16.5"
												RichText="Blunt" />
										</StackPanel>
										<StackPanel x:Uid="Pierce" Orientation="Horizontal">
											<Image Source="/Json/Skills Display Info manager/(Serious area do not enter without caution)/Damage types/Pierce.png" />
											<Terminus:UITranslation_Rose
												UID="[Skills DI Manager] * Damage type — Pierce"
												FontSize="16.5"
												RichText="Pierce" />
										</StackPanel>
										<StackPanel x:Uid="Slash" Orientation="Horizontal">
											<Image Source="/Json/Skills Display Info manager/(Serious area do not enter without caution)/Damage types/Slash.png" />
											<Terminus:UITranslation_Rose
												UID="[Skills DI Manager] * Damage type — Slash"
												FontSize="16.5"
												RichText="Slash" />
										</StackPanel>
										<ComboBoxItem x:Name="NoneDamageTypeOption" Visibility="Collapsed">
											<StackPanel x:Uid="None" Opacity="0.45" Orientation="Horizontal">
												<Canvas Width="19">
													<TextBlock Margin="0.5 -3.5 0 0" FontSize="19" Text="⌀" FontFamily="Segoe UI" />
												</Canvas>
												<Terminus:UITranslation_Rose
													UID="[Skills DI Manager] * Damage type — None"
													FontSize="16.5"
													RichText="None" />
											</StackPanel>
										</ComboBoxItem>
									</ComboBox>
								</Grid>
								<Grid Margin="0 5 0 0">
									<Grid.ColumnDefinitions>
										<ColumnDefinition />
										<ColumnDefinition />
									</Grid.ColumnDefinitions>
									<Terminus:UITranslation_Rose
										UID="[Skills DI Manager] * Affinity (Label)"
										VerticalAlignment="Center"
										PerfectVerticalAlign="True"
										x:Name="AffinitySelectLabel"
										FontFamily="/UI/Fonts/#GOST type BU"
										FontSize="20"
										Foreground="#F5F5DC"
										RichText="Affinity" />
									<ComboBox x:Name="AffinitySelector" Grid.Column="1" SelectedIndex="0" SelectionChanged="AffinitySelector_SelectionChanged">
										<ComboBox.Resources>
											<Style TargetType="{x:Type Terminus:UITranslation_Rose}">
												<Setter Property="Foreground" Value="#F5F5DC" />
												<Setter Property="FontFamily" Value="{Binding ElementName=AffinitySelectLabel, Path=FontFamily}" />
												<Setter Property="FontWeight" Value="{Binding ElementName=AffinitySelectLabel, Path=FontWeight}" />
											</Style>
										</ComboBox.Resources>
										<StackPanel x:Uid="Wrath" Orientation="Horizontal">
											<Image Source="/UI/Limbus/Skills/Affinity Icons/Wrath.png" />
											<Terminus:UITranslation_Rose
												UID="[Skills DI Manager] * Affinity — Wrath"
												FontSize="16.5"
												RichText="Wrath" />
										</StackPanel>
										<StackPanel x:Uid="Lust" Orientation="Horizontal">
											<Image Source="/UI/Limbus/Skills/Affinity Icons/Lust.png" />
											<Terminus:UITranslation_Rose
												UID="[Skills DI Manager] * Affinity — Lust"
												FontSize="16.5"
												RichText="Lust" />
										</StackPanel>
										<StackPanel x:Uid="Sloth" Orientation="Horizontal">
											<Image Source="/UI/Limbus/Skills/Affinity Icons/Sloth.png" />
											<Terminus:UITranslation_Rose
												UID="[Skills DI Manager] * Affinity — Sloth"
												FontSize="16.5"
												RichText="Sloth" />
										</StackPanel>
										<StackPanel x:Uid="Gluttony" Orientation="Horizontal">
											<Image Source="/UI/Limbus/Skills/Affinity Icons/Gluttony.png" />
											<Terminus:UITranslation_Rose
												UID="[Skills DI Manager] * Affinity — Gluttony"
												FontSize="16.5"
												RichText="Gluttony" />
										</StackPanel>
										<StackPanel x:Uid="Gloom" Orientation="Horizontal">
											<Image Source="/UI/Limbus/Skills/Affinity Icons/Gloom.png" />
											<Terminus:UITranslation_Rose
												UID="[Skills DI Manager] * Affinity — Gloom"
												FontSize="16.5"
												RichText="Gloom" />
										</StackPanel>
										<StackPanel x:Uid="Pride" Orientation="Horizontal">
											<Image Source="/UI/Limbus/Skills/Affinity Icons/Pride.png" />
											<Terminus:UITranslation_Rose
												UID="[Skills DI Manager] * Affinity — Pride"
												FontSize="16.5"
												RichText="Pride" />
										</StackPanel>
										<StackPanel x:Uid="Envy" Orientation="Horizontal">
											<Image Source="/UI/Limbus/Skills/Affinity Icons/Envy.png" />
											<Terminus:UITranslation_Rose
												UID="[Skills DI Manager] * Affinity — Envy"
												FontSize="16.5"
												RichText="Envy" />
										</StackPanel>
										<StackPanel x:Uid="None" Orientation="Horizontal">
											<Canvas Width="19">
												<TextBlock Margin="0.5 -3.5 0 0" FontSize="19" Text="⌀" FontFamily="Segoe UI" />
											</Canvas>
											<Terminus:UITranslation_Rose
												UID="[Skills DI Manager] * Affinity — None"
												FontSize="16.5"
												RichText="None" />
										</StackPanel>
									</ComboBox>
								</Grid>
							</StackPanel>
							<Grid Margin="0 5 0 0">
								<Grid.ColumnDefinitions>
									<ColumnDefinition />
									<ColumnDefinition />
								</Grid.ColumnDefinitions>
								<Terminus:UITranslation_Rose
									x:Name="SkillRankSelectLabel"
									VerticalAlignment="Center"
									PerfectVerticalAlign="True"
									UID="[Skills DI Manager] * Rank (Label)"
									FontFamily="/UI/Fonts/#GOST type BU"
									FontSize="20"
									Foreground="#F5F5DC"
									RichText="Rank" />
								<ComboBox x:Name="SkillRankSelector" Grid.Column="1" SelectedIndex="0" SelectionChanged="SkillRankSelector_SelectionChanged">
									<ComboBox.Resources>
										<Style TargetType="{x:Type Terminus:UITranslation_Rose}">
											<Setter Property="Foreground" Value="#F5F5DC" />
											<Setter Property="FontFamily" Value="{Binding ElementName=DamageTypeSelectLabel, Path=FontFamily}" />
											<Setter Property="FontWeight" Value="{Binding ElementName=DamageTypeSelectLabel, Path=FontWeight}" />
										</Style>
									</ComboBox.Resources>
									<Terminus:UITranslation_Rose
										UID="[Skills DI Manager] * Rank — 1"
										x:Uid="1"
										Margin="1 0 0 0"
