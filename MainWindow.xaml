					<TickBar
						x:Name="BottomTick"
						Grid.Column="2"
						Width="4"
						Margin="2 0 0 0"
						Fill="{TemplateBinding Foreground}"
						Placement="Right"
						Visibility="Collapsed" />
					<Border
						x:Name="TrackBackground"
						Grid.Column="1"
						Width="4.0"
						Margin="0 5"
						HorizontalAlignment="center"
						Background="{DynamicResource UITheme_SliderLine_Background}"
						BorderBrush="{DynamicResource UITheme_SliderLine_Border}"
						BorderThickness="1">
						<Canvas Margin="-1 -6">
							<Rectangle x:Name="PART_SelectionRange" Width="4.0" Fill="{DynamicResource {x:Static SystemColors.HighlightBrushKey}}" Visibility="Hidden" />
						</Canvas>
					</Border>
					<Track x:Name="PART_Track" Grid.Column="1">
						<Track.DecreaseRepeatButton>
							<RepeatButton Command="{x:Static Slider.DecreaseLarge}" Style="{StaticResource RepeatButtonTransparent}" />
