<Window
	x:Class="LC_Localization_Task_Absolute.MainWindow"
	xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
	xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
	xmlns:Limbus="clr-namespace:LC_Localization_Task_Absolute.Limbus_Integration"
	xmlns:local="clr-namespace:LC_Localization_Task_Absolute"
	xmlns:Terminus="clr-namespace:LC_Localization_Task_Absolute"
	xmlns:sys="clr-namespace:System;assembly=mscorlib"
	xmlns:theme="clr-namespace:Microsoft.Windows.Themes;assembly=PresentationFramework.Aero2"
	Title="LC Localization Interface"
	Width="1535"
	Height="816"
	MinWidth="710.5"
	MinHeight="425"
	MaxWidth="1920"
	AllowsTransparency="True"
	Loaded="Window_Loaded"
	Opacity="1"
	PreviewKeyDown="Window_PreviewKeyDown"
	PreviewKeyUp="Window_PreviewKeyUp"
	ResizeMode="CanResizeWithGrip"
	SizeChanged="Window_SizeChanged"
	Topmost="True"
	WindowStartupLocation="CenterScreen"
	WindowStyle="None">
	<Window.Triggers>
		<EventTrigger RoutedEvent="Window.Loaded">
			<BeginStoryboard>
				<Storyboard>
					<DoubleAnimation
						AutoReverse="False"
						RepeatBehavior="1x"
						SpeedRatio="4.86"
						Storyboard.TargetProperty="(Window.Opacity)"
						From="0.0"
						To="1.0"
						Duration="0:0:0:01" />
				</Storyboard>
			</BeginStoryboard>
		</EventTrigger>
	</Window.Triggers>
	<WindowChrome.WindowChrome>
		<WindowChrome CaptionHeight="1" ResizeBorderThickness="0" />
	</WindowChrome.WindowChrome>

	<Window.Resources>
		<!--#region UI Theme keys-->

		<!--#region UI Theme / Common-->
		<SolidColorBrush x:Key="Theme_Common__RightMenuNavigationPanelBackground" Color="#00000000" />
		<SolidColorBrush x:Key="Theme_Common__RightMenuObjectNameForeground" Color="#EBCAA2" />
		<!--#endregion-->

		<!--#region UI Theme / UI Text-->
		<SolidColorBrush x:Key="Theme_UIText__Foreground" Color="#A69885" />
		<SolidColorBrush x:Key="Theme_UIText__Foreground_IdentityOrEGOPreviewCreator" Color="#F5F5DC" />
		<!--#endregion-->

		<!--#region UI Theme / UI Textfields-->
		<SolidColorBrush x:Key="Theme_UITextfields__Foreground" Color="#B1B1B9" />
		<SolidColorBrush x:Key="Theme_UITextfields__Foreground_ShadowText" Color="#514C46" />

		<SolidColorBrush x:Key="Theme_UITextfields__Background" Color="#E5241C1C" />

		<SolidColorBrush x:Key="Theme_UITextfields__Border" Color="#483C41" />
		<CornerRadius    x:Key="Theme_UITextfields__Border_CornerRadius">0</CornerRadius>
		<Thickness       x:Key="Theme_UITextfields__Border_Thickness">1.7</Thickness>
		
		<SolidColorBrush x:Key="Theme_UITextfields__Selection" Color="#C4B0B6" />
		<SolidColorBrush x:Key="Theme_UITextfields__Caret" Color="#C4B0B6" />
		<!--#endregion-->

		<!--#region UI Theme / Json Text Editor-->
		<sys:Boolean     x:Key="Theme_JsonTextEditor__ShowLineNumbers">False</sys:Boolean>

		<SolidColorBrush x:Key="Theme_JsonTextEditor__Foreground" Color="#ABA193" />
		<SolidColorBrush x:Key="Theme_JsonTextEditor__Background" Color="#191919" />
		
		<FontFamily      x:Key="Theme_JsonTextEditor__Font">Consolas</FontFamily>
		<FontWeight      x:Key="Theme_JsonTextEditor__FontWeight">Regular</FontWeight>
		<sys:Double      x:Key="Theme_JsonTextEditor__FontSize">16.8</sys:Double>

		<SolidColorBrush x:Key="Theme_JsonTextEditor__Caret" Color="#E9DBC6"/>

		<SolidColorBrush x:Key="Theme_JsonTextEditor__Border" Color="#333333"/>
		<CornerRadius    x:Key="Theme_JsonTextEditor__Border_CornerRadius">0</CornerRadius>
		<Thickness       x:Key="Theme_JsonTextEditor__Border_Thickness">1</Thickness>

		<SolidColorBrush x:Key="Theme_JsonTextEditor__Selection__Foreground" Color="#ABA193"/>
		<SolidColorBrush x:Key="Theme_JsonTextEditor__Selection__Background" Color="#55B4B4B4"/>
		<SolidColorBrush x:Key="Theme_JsonTextEditor__Selection__Border" Color="#66D4D4D4"/>
		<sys:Double      x:Key="Theme_JsonTextEditor__Selection__Border_Thickness">1</sys:Double>
		<sys:Double      x:Key="Theme_JsonTextEditor__Selection__Border_CornerRadius">3.5</sys:Double>
		<!--#endregion-->

		<!--#region UI Theme / Square Buttons-->
		<SolidColorBrush x:Key="Theme_SquareButtons__DisabledButtonsShadowing" Color="#77000000"/>
		
		<SolidColorBrush x:Key="Theme_SquareButtons__Icons" Color="#C4B0B6"/>
		
		<SolidColorBrush x:Key="Theme_SquareButtons__Background" Color="#181818"/>
		<SolidColorBrush x:Key="Theme_SquareButtons__Background_Highlighted" Color="#282828"/>

		<SolidColorBrush x:Key="Theme_SquareButtons__Border" Color="#333333"/>
		<Thickness       x:Key="Theme_SquareButtons__Border_Thickness">1.7</Thickness>
		<CornerRadius    x:Key="Theme_SquareButtons__Border_CornerRadius">0</CornerRadius>
		<!--#endregion-->

		<!--#region UI Theme / Elongated Buttons-->
		<SolidColorBrush x:Key="Theme_ElongatedButtons__DisabledButtonsShadowing" Color="#77000000"/>

		<SolidColorBrush x:Key="Theme_ElongatedButtons__Text" Color="#C4B0B6"/>

		<SolidColorBrush x:Key="Theme_ElongatedButtons__Background" Color="#181818"/>
		<SolidColorBrush x:Key="Theme_ElongatedButtons__Background_Highlighted" Color="#282828"/>

		<SolidColorBrush x:Key="Theme_ElongatedButtons__Border" Color="#333333"/>
		<Thickness       x:Key="Theme_ElongatedButtons__Border_Thickness">1.7</Thickness>
		<CornerRadius    x:Key="Theme_ElongatedButtons__Border_CornerRadius">0</CornerRadius>
		<!--#endregion-->

		<!--#region UI Theme / Other Border-like things-->
		<SolidColorBrush x:Key="Theme_OtherBorderLikeThings__Background" Color="#181818"/>
		
		<SolidColorBrush x:Key="Theme_OtherBorderLikeThings__Border" Color="#333333"/>
		<Thickness       x:Key="Theme_OtherBorderLikeThings__Border_Thickness">1.7</Thickness>
		<CornerRadius    x:Key="Theme_OtherBorderLikeThings__Border_CornerRadius">0</CornerRadius>
		<!--#endregion-->
		

		<!--#region UI Theme / Styles-->
		<Style x:Key="Theme_SquareButton_STYLE" TargetType="Border">
			<Setter Property="Background" Value="{DynamicResource Theme_SquareButtons__Background}" />
			<Setter Property="BorderBrush" Value="{DynamicResource Theme_SquareButtons__Border}" />
			<Setter Property="CornerRadius" Value="{DynamicResource Theme_SquareButtons__Border_CornerRadius}" />
			<Setter Property="BorderThickness" Value="{DynamicResource Theme_SquareButtons__Border_Thickness}" />
			<Style.Triggers>
				<Trigger Property="IsMouseOver" Value="True">
					<Setter Property="Background" Value="{DynamicResource Theme_SquareButtons__Background_Highlighted}" />
				</Trigger>
			</Style.Triggers>
		</Style>

		<Style x:Key="Theme_ElongatedButton_STYLE" TargetType="Border">
			<Setter Property="Background" Value="{DynamicResource Theme_ElongatedButtons__Background}" />
			<Setter Property="BorderBrush" Value="{DynamicResource Theme_ElongatedButtons__Border}" />
			<Setter Property="CornerRadius" Value="{DynamicResource Theme_ElongatedButtons__Border_CornerRadius}" />
			<Setter Property="BorderThickness" Value="{DynamicResource Theme_ElongatedButtons__Border_Thickness}" />
			<Style.Triggers>
				<Trigger Property="IsMouseOver" Value="True">
					<Setter Property="Background" Value="{DynamicResource Theme_ElongatedButtons__Background_Highlighted}" />
				</Trigger>
			</Style.Triggers>
		</Style>
		
		

		<Style x:Key="Theme_SquareButtons_DisabledButtonShadowing_STYLE" TargetType="Border">
			<Setter Property="Background" Value="{DynamicResource Theme_SquareButtons__DisabledButtonsShadowing}"/>
			<Setter Property="CornerRadius" Value="{DynamicResource Theme_SquareButtons__Border_CornerRadius}"/>
			<Setter Property="Panel.ZIndex" Value="1"/>
			<Setter Property="BorderThickness" Value="0"/>
		</Style>

		<Style x:Key="Theme_ElongatedButtons_DisabledButtonShadowing_STYLE" TargetType="Border">
			<Setter Property="Background" Value="{DynamicResource Theme_ElongatedButtons__DisabledButtonsShadowing}"/>
			<Setter Property="CornerRadius" Value="{DynamicResource Theme_ElongatedButtons__Border_CornerRadius}"/>
			<Setter Property="Panel.ZIndex" Value="1"/>
			<Setter Property="BorderThickness" Value="0"/>
		</Style>

		<Style x:Key="Theme_Textfield_STYLE" TargetType="Terminus:UITranslation_Mint"> <!--TextBox style-->
			<Setter Property="Background"      Value="Transparent" />
			<Setter Property="BorderThickness" Value="0" />
			<Setter Property="FontSize"        Value="20" />
			
			<Setter Property="Foreground"     Value="{DynamicResource Theme_UITextfields__Foreground}" />
			<Setter Property="CaretBrush"     Value="{DynamicResource Theme_UITextfields__Caret}" />
			<Setter Property="SelectionBrush" Value="{DynamicResource Theme_UITextfields__Selection}" />
		</Style>
		<Style x:Key="Theme_Textfield_Border_STYLE" TargetType="Border">
			<!--Border around TextBox style-->
			<Setter Property="Background"      Value="{DynamicResource Theme_UITextfields__Background}" />
			<Setter Property="BorderBrush"     Value="{DynamicResource Theme_UITextfields__Border}" />
			<Setter Property="CornerRadius"    Value="{DynamicResource Theme_UITextfields__Border_CornerRadius}" />
			<Setter Property="BorderThickness" Value="{DynamicResource Theme_UITextfields__Border_Thickness}" />
		</Style>

		<Style x:Key="Theme_OtherBorderLikeThing" TargetType="Border">
			<Setter Property="Background"      Value="{DynamicResource Theme_OtherBorderLikeThings__Background}" />
			<Setter Property="BorderBrush"     Value="{DynamicResource Theme_OtherBorderLikeThings__Border}" />
			<Setter Property="CornerRadius"    Value="{DynamicResource Theme_OtherBorderLikeThings__Border_CornerRadius}" />
			<Setter Property="BorderThickness" Value="{DynamicResource Theme_OtherBorderLikeThings__Border_Thickness}" />
		</Style>
		<!--#endregion-->

		<!--#endregion-->
		

		<FontFamily x:Key="BebasKaiUniversal">pack://application:,,,/UI/Fonts/#Bebas Neue Cyrillic</FontFamily>

		<!-- 
			Special font that turns all text into squares to prevent usage of fonts that are not unity font asset from Limbus Company if attaching <font> tag at limbus text
			(`[⇲] Assets Directory\[⇲] Limbus Embedded Fonts` folder)
			
			Allowed only fonts like <font="EN/cur)Caveat-SemiBold/Caveat-SemiBold SDF">, <font="EN/title)mikodacs/Mikodacs SDF"> and etc.
			See `[⇲] Assets Directory\[⇲] Limbus Embedded Fonts\Usage.txt` for full list of supported game fonts.
		-->
		<FontFamily x:Key="SegoeFluentIcons_MakeLimbusPreviewInvalid">pack://application:,,,/UI/Fonts/#Segoe Fluent Icons</FontFamily>

		
		<!--#region all the styles except slider-->
		<Style x:Key="TB_ShutdownHighlight" TargetType="Border">
			<Setter Property="Background" Value="Transparent" />
			<Style.Triggers>
				<Trigger Property="IsMouseOver" Value="True">
					<Setter Property="Background" Value="#FFA52C42" />
				</Trigger>
			</Style.Triggers>
		</Style>
		<Style x:Key="TB_Highlight" TargetType="Border">
			<Setter Property="Background" Value="Transparent" />
			<Style.Triggers>
				<Trigger Property="IsMouseOver" Value="True">
					<Setter Property="Background" Value="#FF302829" />
				</Trigger>
			</Style.Triggers>
		</Style>

		<Style x:Key="ButtonDefaultDeskHighlightableDefault" TargetType="Border">
			<Setter Property="Background" Value="#FF202020" />
			<Setter Property="BorderBrush" Value="#393939" />
			<Setter Property="CornerRadius" Value="1" />
			<Setter Property="BorderThickness" Value="1" />
			<Style.Triggers>
				<Trigger Property="IsMouseOver" Value="True">
					<Setter Property="Background" Value="#FF272529" />
				</Trigger>
			</Style.Triggers>
		</Style>
		
		<Style x:Key="CoinDesc" TargetType="Limbus:TMProEmitter">
			<Setter Property="FontSize" Value="20" />
			<Setter Property="Background" Value="Transparent" />
			<Setter Property="Foreground" Value="#ebcaa2" />
			<EventSetter Event="PreviewMouseRightButtonDown" Handler="FastSwitch_ToSkillCoinDesc" />
		</Style>

		
		<!--#region UI CONTROLTEMPLATES BIG TEXTBIGTEXTBIGTEPIN-->
		
		<Style x:Key="{x:Type ContextMenu}" TargetType="{x:Type ContextMenu}">
			<Setter Property="OverridesDefaultStyle" Value="True" />
			<Setter Property="SnapsToDevicePixels" Value="True" />
			<Setter Property="Template">
				<Setter.Value>
					<ControlTemplate TargetType="{x:Type ContextMenu}">
						<Border
							Background="{DynamicResource Theme_OtherBorderLikeThings__Background}"
							BorderBrush="{DynamicResource Theme_OtherBorderLikeThings__Border}"
							BorderThickness="2"
							CornerRadius="{DynamicResource Theme_OtherBorderLikeThings__Border_CornerRadius}"
							Opacity="0.96">
							<StackPanel Margin="1.3" IsItemsHost="True" Orientation="Vertical" />
						</Border>
					</ControlTemplate>
				</Setter.Value>
			</Setter>
		</Style>
		<Style TargetType="MenuItem">
			<Setter Property="Template" Value="{DynamicResource MenuItemControlTemplate}" />
			<Setter Property="FontSize" Value="16" />
			<Setter Property="Margin" Value="0 0 0 0" />
			<Style.Triggers>
				<Trigger Property="IsMouseOver" Value="True" />
			</Style.Triggers>
		</Style>

		<ControlTemplate x:Key="MenuItemControlTemplate" TargetType="{x:Type MenuItem}">
			<Border
				x:Name="templateRoot"
				BorderThickness="0"
				CornerRadius="{DynamicResource Theme_ElongatedButtons__Border_CornerRadius}"
				SnapsToDevicePixels="True">
				<Grid VerticalAlignment="Center">
					<Grid.ColumnDefinitions>
						<ColumnDefinition Width="5" />
						<ColumnDefinition />
					</Grid.ColumnDefinitions>
					<ContentPresenter
						Grid.Column="1"
						Content="{TemplateBinding Header}"
						ContentSource="Header"
						ContentStringFormat="{TemplateBinding HeaderStringFormat}"
						ContentTemplate="{TemplateBinding HeaderTemplate}"
						RecognizesAccessKey="True"
						SnapsToDevicePixels="{TemplateBinding SnapsToDevicePixels}" />
					<Popup
						x:Name="PART_Popup"
						AllowsTransparency="True"
						Focusable="False"
						IsOpen="{Binding IsSubmenuOpen, RelativeSource={RelativeSource TemplatedParent}}"
						Placement="Bottom"
						PopupAnimation="{DynamicResource {x:Static SystemParameters.MenuPopupAnimationKey}}">
						<Border
							x:Name="SubMenuBorder"
							Padding="0"
							Background="{DynamicResource Theme_ElongatedButtons__Background}"
							BorderBrush="{DynamicResource Theme_ElongatedButtons__Border}"
							BorderThickness="1">
							<ScrollViewer x:Name="SubMenuScrollViewer" Style="{DynamicResource {ComponentResourceKey ResourceId=MenuScrollViewer, TypeInTargetAssembly={x:Type FrameworkElement}}}">
								<Grid RenderOptions.ClearTypeHint="Enabled">
									<Canvas Width="0" Height="0" HorizontalAlignment="Left">
										<Rectangle x:Name="OpaqueRect" Width="{Binding ActualWidth, ElementName=SubMenuBorder}" Height="{Binding ActualHeight, ElementName=SubMenuBorder}" Fill="{Binding Background, ElementName=SubMenuBorder}" />
									</Canvas>
									<ItemsPresenter
										x:Name="ItemsPresenter"
										Grid.IsSharedSizeScope="True"
										KeyboardNavigation.DirectionalNavigation="Cycle"
										KeyboardNavigation.TabNavigation="Cycle"
										SnapsToDevicePixels="{TemplateBinding SnapsToDevicePixels}" />
								</Grid>
							</ScrollViewer>
						</Border>
					</Popup>
				</Grid>
			</Border>
			<ControlTemplate.Triggers>
				<Trigger Property="IsMouseOver" Value="True">
					<Setter TargetName="templateRoot" Property="BorderBrush" Value="{DynamicResource Theme_ElongatedButtons__Background}" />
					<Setter TargetName="templateRoot" Property="BorderThickness" Value="0" />
				</Trigger>

				<Trigger SourceName="SubMenuScrollViewer" Property="CanContentScroll" Value="False">
					<Setter TargetName="OpaqueRect" Property="Canvas.Top" Value="{Binding VerticalOffset, ElementName=SubMenuScrollViewer}" />
					<Setter TargetName="OpaqueRect" Property="Canvas.Left" Value="{Binding HorizontalOffset, ElementName=SubMenuScrollViewer}" />
				</Trigger>

				<Trigger Property="IsMouseOver" Value="True">
					<Setter TargetName="templateRoot" Property="Background" Value="{DynamicResource Theme_ElongatedButtons__Background_Highlighted}" />
				</Trigger>
			</ControlTemplate.Triggers>
		</ControlTemplate>

		<Style x:Key="ContextMenuItem_r" TargetType="Terminus:UITranslation_Rose">
			<Setter Property="Padding" Value="0 2.5 0 2.5" />
			<Setter Property="FontSize" Value="20" />
		</Style>


		<Style x:Key="ScrollBarLineButton" TargetType="{x:Type RepeatButton}">
			<Setter Property="SnapsToDevicePixels" Value="True" />
			<Setter Property="OverridesDefaultStyle" Value="True" />
			<Setter Property="Focusable" Value="False" />
			<Setter Property="Template">
				<Setter.Value>
					<ControlTemplate TargetType="{x:Type RepeatButton}">
						<Border x:Name="Border" Background="Transparent" BorderThickness="0" CornerRadius="0">
							<Path
								x:Name="Arrow"
								HorizontalAlignment="Center"
								VerticalAlignment="Center"
								Data="{Binding Content, RelativeSource={RelativeSource TemplatedParent}}"
								RenderTransformOrigin="0.5 0.5">
								<Path.Fill>
									<SolidColorBrush Color="#999999" />
								</Path.Fill>
								<Path.RenderTransform>
									<ScaleTransform ScaleX="1.2" ScaleY="1.2" />
								</Path.RenderTransform>
							</Path>
							<VisualStateManager.VisualStateGroups>
								<VisualStateGroup x:Name="CommonStates">
									<VisualState x:Name="Normal" />
									<VisualState x:Name="MouseOver">
										<Storyboard>
											<ColorAnimationUsingKeyFrames Storyboard.TargetProperty="(Panel.Background).(SolidColorBrush.Color)">
												<EasingColorKeyFrame KeyTime="0" Value="#4d4d4d" />
											</ColorAnimationUsingKeyFrames>
										</Storyboard>
									</VisualState>
								</VisualStateGroup>
							</VisualStateManager.VisualStateGroups>
						</Border>
					</ControlTemplate>
				</Setter.Value>
			</Setter>
		</Style>
		<Style x:Key="ScrollBarPageButton" TargetType="{x:Type RepeatButton}">
			<Setter Property="SnapsToDevicePixels" Value="True" />
			<Setter Property="OverridesDefaultStyle" Value="True" />
			<Setter Property="IsTabStop" Value="False" />
			<Setter Property="Focusable" Value="False" />

		</Style>
		<Style x:Key="ScrollBarThumb" TargetType="{x:Type Thumb}">
			<Setter Property="SnapsToDevicePixels" Value="True" />
			<Setter Property="OverridesDefaultStyle" Value="True" />
			<Setter Property="IsTabStop" Value="False" />
			<Setter Property="Focusable" Value="False" />
			<Setter Property="Template">
				<Setter.Value>
					<ControlTemplate TargetType="{x:Type Thumb}">
						<Border Background="#4d4d4d" BorderBrush="{TemplateBinding BorderBrush}" BorderThickness="1" CornerRadius="0" />
					</ControlTemplate>
				</Setter.Value>
			</Setter>
		</Style>
		<ControlTemplate x:Key="VerticalScrollBar" TargetType="{x:Type ScrollBar}">
			<Grid>
				<Grid.RowDefinitions>
					<RowDefinition MaxHeight="18" />
					<RowDefinition Height="0.00001*" />
					<RowDefinition MaxHeight="18" />
				</Grid.RowDefinitions>
				<Border Grid.RowSpan="3" Background="#2e2e2e" CornerRadius="0" />
				<RepeatButton
					Grid.Row="0"
					Height="18"
					Command="ScrollBar.LineUpCommand"
					Content="M 0 4 L 8 4 L 4 0 Z"
					Style="{StaticResource ScrollBarLineButton}" />
				<Track x:Name="PART_Track" Grid.Row="1" IsDirectionReversed="True">
					<Track.DecreaseRepeatButton>
						<RepeatButton Command="ScrollBar.PageUpCommand" Style="{StaticResource ScrollBarPageButton}" />
					</Track.DecreaseRepeatButton>
					<Track.Thumb>
						<Thumb Margin="2.5 -1 3 -1" Style="{StaticResource ScrollBarThumb}" />
					</Track.Thumb>
					<Track.IncreaseRepeatButton>
						<RepeatButton Command="ScrollBar.PageDownCommand" Style="{StaticResource ScrollBarPageButton}" />
					</Track.IncreaseRepeatButton>
				</Track>
				<RepeatButton
					Grid.Row="2"
					Height="18"
					Command="ScrollBar.LineDownCommand"
					Content="M 0 0 L 4 4 L 8 0 Z"
					Style="{StaticResource ScrollBarLineButton}" />
			</Grid>
		</ControlTemplate>
		<ControlTemplate x:Key="HorizontalScrollBar" TargetType="{x:Type ScrollBar}">
			<Grid>
				<Grid.ColumnDefinitions>
					<ColumnDefinition MaxWidth="18" />
					<ColumnDefinition Width="0.00001*" />
					<ColumnDefinition MaxWidth="18" />
				</Grid.ColumnDefinitions>
				<Border Grid.ColumnSpan="3" Background="#2e2e2e" />
				<RepeatButton
					Grid.Column="0"
					Width="18"
					Command="ScrollBar.LineLeftCommand"
					Content="M 4 0 L 4 8 L 0 4 Z"
					Style="{StaticResource ScrollBarLineButton}" />
				<Track x:Name="PART_Track" Grid.Column="1" IsDirectionReversed="False">
					<Track.DecreaseRepeatButton>
						<RepeatButton Command="ScrollBar.PageLeftCommand" Style="{StaticResource ScrollBarPageButton}" />
					</Track.DecreaseRepeatButton>
					<Track.Thumb>
						<Thumb Margin="-1 2.5 -1 3" Style="{StaticResource ScrollBarThumb}" />
					</Track.Thumb>
					<Track.IncreaseRepeatButton>
						<RepeatButton Command="ScrollBar.PageRightCommand" Style="{StaticResource ScrollBarPageButton}" />
					</Track.IncreaseRepeatButton>
				</Track>
				<RepeatButton
					Grid.Column="2"
					Width="18"
					Command="ScrollBar.LineRightCommand"
					Content="M 0 0 L 4 4 L 0 8 Z"
					Style="{StaticResource ScrollBarLineButton}" />
			</Grid>
		</ControlTemplate>
		<Style x:Key="SeriousScrollBar" TargetType="{x:Type ScrollBar}">
			<Setter Property="SnapsToDevicePixels" Value="True" />
			<Setter Property="OverridesDefaultStyle" Value="True" />
			<Setter Property="ContextMenu" Value="{x:Null}" />
			<Style.Triggers>
				<Trigger Property="Orientation" Value="Horizontal">
					<Setter Property="Width" Value="Auto" />
					<Setter Property="Height" Value="18" />
					<Setter Property="Template" Value="{StaticResource HorizontalScrollBar}" />
				</Trigger>
				<Trigger Property="Orientation" Value="Vertical">
					<Setter Property="Width" Value="18" />
					<Setter Property="Height" Value="Auto" />
					<Setter Property="Template" Value="{StaticResource VerticalScrollBar}" />
				</Trigger>
			</Style.Triggers>
		</Style>
		

		

		
		<Style x:Key="RepeatButtonTransparent" TargetType="{x:Type RepeatButton}">
			<Setter Property="OverridesDefaultStyle" Value="true" />
			<Setter Property="Background" Value="Transparent" />
			<Setter Property="Focusable" Value="false" />
			<Setter Property="IsTabStop" Value="false" />
			<Setter Property="Template">
				<Setter.Value>
					<ControlTemplate TargetType="{x:Type RepeatButton}">
						<Rectangle Width="{TemplateBinding Width}" Height="{TemplateBinding Height}" Fill="{TemplateBinding Background}" />
					</ControlTemplate>
				</Setter.Value>
			</Setter>
		</Style>

		
		<SolidColorBrush x:Key="UITheme_SliderThumb_DefaultBackground" Color="#b8b3b1" />
		<SolidColorBrush x:Key="UITheme_SliderThumb_DefaultBorder" Color="#888381" />

		<SolidColorBrush x:Key="UITheme_SliderThumb_HighlightedBackground" Color="#888381" />
		<SolidColorBrush x:Key="UITheme_SliderThumb_HighlightedBorder" Color="#888381" />

		<SolidColorBrush x:Key="UITheme_SliderThumb_PressedBackground" Color="#888381" />
		<SolidColorBrush x:Key="UITheme_SliderThumb_PressedBorder" Color="#888381" />

		<CornerRadius x:Key="UITheme_SliderThumb_CornerRadius">0</CornerRadius>
		<Thickness x:Key="UITheme_SliderThumb_BorderThickness">0.8</Thickness>
		<sys:Double x:Key="UITheme_SliderThumb_Width">8</sys:Double>
		<sys:Double x:Key="UITheme_SliderThumb_Height">12</sys:Double>
		<sys:Double x:Key="UITheme_SliderThumb_Scale">1.45</sys:Double>

		<sys:Double x:Key="UITheme_SliderLine_Height">2.5</sys:Double>
		<Thickness x:Key="UITheme_SliderLine_BorderThickness">1.45</Thickness>

		<SolidColorBrush x:Key="UITheme_SliderLine_Background" Color="#b0adac" />

		<SolidColorBrush x:Key="UITheme_SliderLine_Border" Color="#a6a09e" />


		<SolidColorBrush x:Key="SliderThumb.Static.Foreground" Color="#a6a09e" />
		<SolidColorBrush x:Key="SliderThumb.Disabled.Background" Color="#FFF0F0F0" />
